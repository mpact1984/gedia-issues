contentDir 'content/'
contentExt '.content'
outputDir 'output/'
outputExt '.html'
scriptExt '.py'
defaultTemplate 'template/page.template'

backupScripts 1

lolcatDefault 0

lolcatCmd 'nift lolcat-cc -f'

buildThreads -1

paginateThreads -1

incrementalMode -2022

terminal ''

unixTextEditor 'nano'
winTextEditor 'notepad'

rootBranch 'stage'
outputBranch 'master'
